  <row>
    <panel>
      <title>$user_onid$ LB exmail hits (24hr)</title>
      <table>
        <search>
          <query>sourcetype=citrix:netscaler:ipfix netscalerHttpDomainName="exmail.oregonstate.edu" User=*$user_onid$* | stats count as hits by Cmd | addcoltotals labelfield=Cmd label=Total</query>
          <earliest>-24h@m</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
      </table>
    </panel>
    <panel>
      <title>$user_onid$ average time_taken (24hr)</title>
      <table>
        <search>
          <query>sourcetype=iis User=*$user_onid$* | stats avg(time_taken) as ms by Cmd | eval ms=round(ms,2)</query>
          <earliest>-24h@m</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
      </table>
    </panel>
    <panel>
      <title>Global average exmail hits per user (24hr)</title>
      <single>
        <search>
          <query>sourcetype=citrix:netscaler:ipfix netscalerHttpDomainName="exmail.oregonstate.edu" User!="" | stats count by User | stats avg(count)</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
