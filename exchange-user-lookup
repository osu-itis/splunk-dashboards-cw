    </input>
  </fieldset>
  <row>
    <panel>
      <title>User agents (7d)</title>
      <table>
        <search>
          <query>sourcetype=iis cs_username=*$user_onid$* | table cs_User_Agent | dedup cs_User_Agent | sort cs_User_Agent</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">50</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
