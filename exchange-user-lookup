      <single>
        <search>
          <query>sourcetype=iis User!="" | stats avg(time_taken) as averages by User | stats avg(averages)</query>
          <earliest>-24h@m</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
