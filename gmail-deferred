<form>
  <label>Gmail messages deferred</label>
  <description>Contains postfix_syslog data, where relay="*l.google.com*" and status=deferred. Totals displayed are not distinct counts.</description>
  <fieldset submitButton="false" autoRun="true">
    <input type="time" searchWhenChanged="true">
      <label>Select date and time</label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Total deferred</title>
      <single>
        <search>
          <query>sourcetype=postfix_syslog relay="*l.google.com*" status=deferred | stats count(status)</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
      </single>
    </panel>
    <panel>
      <title>OSU-Gmail deferred total</title>
      <single>
        <title>('no results found' expected)</title>
        <search>
          <query>sourcetype=postfix_syslog
relay="*l.google.com*"
status=deferred
to="*g-mx.oregonstate.edu"</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <title>Origin IP marked as spammer</title>
      <single>
        <search>
          <query>sourcetype=postfix_syslog
relay="*l.google.com*"
status=deferred
unsolicited
| chart count(status) as "Rejected as spammer"</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
      </single>
    </panel>
    <panel>
      <title>Failed auths</title>
      <single>
        <search>
          <query>sourcetype=postfix_syslog
relay="*l.google.com*"
status=deferred
authentication
| chart count(status) as "Failed authentications"</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
      </single>
    </panel>
    <panel>
      <title>Mailbox over quota</title>
      <single>
        <search>
          <query>sourcetype=postfix_syslog
relay="*l.google.com*"
status=deferred
quota
| chart count(status) as "Over quota"</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </single>
    </panel>
  </row>
