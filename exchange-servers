    <panel>
      <title>500s by host</title>
      <table>
        <search>
          <query>sourcetype=ms:iis:auto sc_status="50*" |stats count(sc_status) as errors by host</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">50</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
    <panel>
      <title>500 substatus'</title>
      <table>
        <title>(this might only shows 0's, try increasing time window)</title>
        <search>
          <query>sourcetype=ms:iis:auto sc_status="50*" |stats count(sc_substatus) as total by sc_status, sc_substatus, sc_win32_status</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">50</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
    <panel>
      <title>Errors by user</title>
      <table>
        <search>
          <query>sourcetype=ms:iis:auto sc_status=5* | stats count(sc_status) as errors by cs_username | sort -errors | head 10 | rex field=cs_username mode=sed "s/^[^\\\]*\\\//g"</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <drilldown>
          <link target="_blank">/app/search/exchange_user_lookup?form.user_onid=$row.cs_username$</link>
        </drilldown>
        <option name="drilldown">row</option>
        <option name="count">50</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
