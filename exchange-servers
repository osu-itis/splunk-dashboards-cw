    <panel>
      <title>non 200/300 statuses</title>
      <table>
        <search>
          <query>sourcetype=ms:iis:auto sc_status!="2*" AND sc_status!="3*" |stats count(sc_substatus) as total by sc_status, sc_substatus, sc_win32_status | sort -total</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">50</option>
        <option name="dataOverlayMode">none</option>
