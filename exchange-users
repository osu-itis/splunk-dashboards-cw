      <chart>
        <search>
          <query>sourcetype=iis cs_username!="" AND DeviceType!="" 
| replace "SAMSUNG*"  with "Android" in DeviceType
| replace "Samsung*" with "Android" in DeviceType 
| replace  "LG*" with Android in DeviceType
| replace "KF*" with "Android" in DeviceType
| replace "Moto*" with "Android" in DeviceType,
| replace "HTC*" with "Android" in DeviceType
| replace "Sony*" with "Android" in DeviceType
| replace "i*" with "iOS" in DeviceType 
| chart dc(cs_username) by DeviceType</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
