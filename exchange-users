  <row>
    <panel>
      <title>Http Traffic</title>
      <chart>
        <search>
          <query>sourcetype="citrix:netscaler:ipfix" AND destinationIPv4Address="128.193.7.94" netscalerHttpReqUrl!="" netscalerHttpReqUrl!="/" netscalerHttpReqUrl!="/favicon*" netscalerHttpReqUrl!="/apple*"
| replace "/rpc*" with "RPC" in netscalerHttpReqUrl
| replace "/owa*" with "OWA" in netscalerHttpReqUrl
| replace "/OWA*" with "OWA" in netscalerHttpReqUrl
| replace "/exchweb*" with "OWA" in netscalerHttpReqUrl
| replace "/rpc*" with "RPC" in netscalerHttpReqUrl
| replace "/OAB*" with "OAB" in netscalerHttpReqUrl
| replace "/oab*" with "OAB" in netscalerHttpReqUrl
| replace "/EWS*" with "EWS" in netscalerHttpReqUrl
| replace "/ews*" with "EWS" in netscalerHttpReqUrl
| replace "*ActiveSync*" with "ActiveSync" in netscalerHttpReqUrl
| replace "/AutoDiscover*" with "AutoDiscover" in netscalerHttpReqUrl
| replace "/autodiscover*" with "AutoDiscover" in netscalerHttpReqUrl
| replace "/Autodiscover*" with "AutoDiscover" in netscalerHttpReqUrl
| replace "/ecp*" with "ECP" in netscalerHttpReqUrl
| replace "/ECP*" with "ECP" in netscalerHttpReqUrl
| where netscalerHttpReqUrl="AutoDiscover" OR netscalerHttpReqUrl="ActiveSync" OR netscalerHttpReqUrl="EWS" OR netscalerHttpReqUrl="RPC" OR netscalerHttpReqUrl="OAB" OR netscalerHttpReqUrl="OWA"
| timechart span=5m count(netscalerHttpReqUrl) as traffic by netscalerHttpReqUrl 
| sort -traffic</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.text">endpoints</option>
