      <title>iPhones/iPads</title>
      <single>
        <search>
          <query>sourcetype=iis DeviceType="iPhone" OR "iPad" |stats count(DeviceType) as device_count | eventstats sum(device_count) AS total | table total</query>
          <earliest>-60m@m</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
